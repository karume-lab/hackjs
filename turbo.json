{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": ["ANDROID_HOME", "JAVA_HOME", "PATH", "USER", "HOME"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "android/app/build/outputs/apk/**", "ios/build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "android": {
      "cache": false,
      "persistent": true
    },
    "ios": {
      "cache": false,
      "persistent": true
    },
    "get-ip": {
      "cache": false
    },
    "install-apk": {
      "cache": false
    },
    "build:install-apk": {
      "cache": false
    },
    "doctor": {
      "cache": false
    },
    "lint": {},
    "typecheck": {}
  }
}
